{
  "BatchSize": 128,
  "WriterCount": 1,
  "Mode": "insert",

  "Source": {
    "Descriptor": "tcp([host]:3000)/test",
    "DriverName": "aerospike",
    "Parameters": {
      "namespace": "test",
      "host": "$aerospikeIP"
    },
    "Query": "SELECT id, name, visited, perf, ARRAY(quiz) AS quiz FROM users"
  },

  "Dest": {
    "DriverName": "bigquery",
    "Credentials": "bq",
    "Parameters": {
      "datasetId": "db2"
    },
    "Table": "users"
  }
}
